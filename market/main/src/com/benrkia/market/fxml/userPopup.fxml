<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import com.jfoenix.controls.JFXButton?>

<VBox prefWidth="100.0" style="-fx-background-color: white; -fx-border-radius: 10;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JFXButton prefWidth="100.0" style="-fx-border-color: #2196f3; -fx-border-width: 0 0 1 0;" text="Profile" textFill="#2196f3" fx:id="profileButton">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton layoutX="10.0" layoutY="10.0" prefWidth="100.0" style="-fx-border-color: #2196f3; -fx-border-width: 0 0 1 0;" text="Logout" textFill="#2196f3" fx:id="logoutButton">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </JFXButton>
   </children>
</VBox>
